<div class="bigspacer"></div>
<!--<i>Click on names to see biographies.</i>-->

<div class="bigspacer"></div>

<div class="row">
	  <div class="col-md-8">
		    <div class="title">
			      Current members
		    </div>
	  </div>
</div>

<div class="bigspacer"></div>

{% for member in site.categories.team reversed %}
{% if member.alumni != true and member.sidekick != true %}
{% cycle 'add rows': '<div class="row">', '<!-- two -->', '<!-- three -->' %}
		<div class="col-md-4 memberbox">
			  <div class="media">

  				  <a class="pull-left" href="{{ site.baseurl }}{{ member.url }}">
    				    <img width=160 class="mr-3" src="{{ site.baseurl }}{{ member.image }}">
  				  </a>
 			 	    <div class="media-body">
    				    <div class="head mb-1"><a href="{{ site.baseurl }}{{ member.url }}" class="off">{{ member.title }}</a></div>
    				    <p class="note">{{ member.position }}</p>
  				  </div>
			  </div>
			  <div class="bigspacer"></div>
			  <div class="bigspacer"></div>
    </div>
    {% cycle 'close rows': '', '', '</div>' %}
    {% endif %}
    {% endfor %}
    {% cycle 'close rows': '', '</div>', '</div>' %}

    <div class="bigspacer"></div>
    <div class="bigspacer"></div>

    <div class="row">
	      <div class="col-md-12">
		        <div class="title">
			          Alumni
		        </div>
	      </div>
    </div>

    <div class="bigspacer"></div>

    {% for member in site.categories.team %}
	  {% if member.alumni == true %}
    {% cycle 'add rows 3': '<div class="row">', '', '' %}
		    <div class="col-md-4 memberbox">
			      <div class="media">
                <a class="pull-left" href="{{ site.baseurl}}{{ member.url }}">
                    <img width=160 class="media-object" src="{{ site.baseurl}}{{ member.image }}">
  				      </a>
 			 	        <div class="media-body">
                    <div class="head media-heading"><a href="{{ site.baseurl}}{{ member.url }}" class="off">{{ member.title }}</a></div>
    				        <p class="note">{{ member.position }}</p>
  				      </div>
			      </div>
			      <div class="bigspacer"></div>
			      <div class="bigspacer"></div>
        </div>
        {% cycle 'close rows 3': '', '', '</div>' %}
        {% endif %}
        {% endfor %}
        {% cycle 'close rows 3': '', '</div>', '</div>' %}

        <div class="bigspacer"></div>
        <div class="bigspacer"></div>

        <div class="row">
	          <div class="col-md-12">
		            <div class="title">
			              Associates
		            </div>
	          </div>
        </div>

        <div class="bigspacer"></div>

        {% for member in site.categories.team reversed %}
	      {% if member.sidekick == true %}
        {% cycle 'add rows 2': '<div class="row">', '', '' %}
		        <div class="col-md-4 memberbox">
			          <div class="media">
                    <a class="pull-left" href="{{ site.baseurl }}{{ member.url }}">
                        <img width=120 class="media-object" src="{{ site.baseurl }}{{ member.image }}">
  				          </a>
 			 	            <div class="media-body">
                        <div class="head media-heading"><a href="{{ site.baseurl }}{{ member.url }}" class="off">{{ member.title }}</a></div>
    				            <p class="note">{{ member.position }}</p>
  				          </div>
			          </div>
			          <div class="bigspacer"></div>
            </div>
            {% cycle 'close rows 2': '', '', '</div>' %}
            {% endif %}
            {% endfor %}
            {% cycle 'close rows 2': '', '</div>', '</div>' %}

